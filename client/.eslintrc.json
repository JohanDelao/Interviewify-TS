{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "root": true,
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "error"
  }
}
